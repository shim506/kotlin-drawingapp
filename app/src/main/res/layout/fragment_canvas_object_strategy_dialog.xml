<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".CanvasObjectStrategyDialogFragment">

    <Button
        android:id="@+id/set_most_back_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="맨 뒤로 보내기"
        android:textColor="@color/drawing_gray"
        android:textStyle="bold" />

    <Button
        android:id="@+id/set_back_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="뒤로 보내기"
        android:textColor="@color/drawing_gray"
        android:textStyle="bold" />

    <Button
        android:id="@+id/set_forth_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="앞으로 보내기"
        android:textColor="@color/drawing_gray"
        android:textStyle="bold" />

    <Button
        android:id="@+id/set_most_forth_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="맨 앞으로 보내기"
        android:textColor="@color/drawing_gray"
        android:textStyle="bold" />
</LinearLayout>